{
  "name": "hrnet",
  "version": "1.0.0",
  "description": "OPC Project 14 HRNet",
  "main": "server.js",
  "scripts": {
    "back:install": "npm install --prefix backend",
    "front:install": "npm install --prefix frontend",
    "install": "concurrently \"npm run back:install\" \"npm run front:install\"",
    "doc": "cd frontend && npm run jsdoc && cd..",
    "server": "npm run dev --prefix backend",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "linter": "npm run lint --prefix frontend",
    "fix:linter": "npm run lint:fix --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laminemessaci/HRNet.git"
  },
